{
  "Do block": {
    "prefix": "do",
    "body": ["do", "\t$0", "end"],
    "description": "Do block"
  },
  "Do param block": {
    "prefix": "do |",
    "body": ["do |$0|", "\t", "end"],
    "description": "Do block"
  },
