  },
  "Let! block": {
    "prefix": "let!",
    "body": ["let!(:$0) {  }"],
    "description": "Let block"
  },
  "Let! do block": {
    "prefix": "let! do",
    "body": ["let!(:$0) do", "\t", "end"],
    "description": "Let block"
  },
  "It do block": {
    "prefix": "it",
